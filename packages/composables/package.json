{
  "name": "@veroui/composables",
  "version": "0.0.1",
  "description": "VeroUI composables for Vue",
  "keywords": [
    "veroui",
    "composables",
    "vue",
    "hooks"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist .turbo",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@veroui/system": "workspace:*",
    "@veroui/theme": "workspace:*",
    "@veroui/vue-utils": "workspace:*",
    "@veroui/shared-utils": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vue": "^3.4.0"
  }
}
