{
    "name": "@veroui/composables",
    "version": "0.0.1",
    "description": "VeroUI composables for Vue",
    "keywords": [
        "veroui",
        "composables",
        "vue",
        "hooks"
    ],
    "license": "MIT",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "rimraf dist .turbo",
        "type-check": "tsc --noEmit"
    },
    "peerDependencies": {
        "vue": "^3.4.0"
    },
    "dependencies": {
        "@veroui/system": "workspace:*",
        "@veroui/theme": "workspace:*",
        "@veroui/vue-utils": "workspace:*",
        "@veroui/shared-utils": "workspace:*",
        "@react-aria/button": "3.14.2",
        "@react-aria/focus": "3.21.2",
        "@react-aria/selection": "3.26.0",
        "@react-stately/tree": "3.9.3",
        "@react-types/accordion": "3.0.0-alpha.26",
        "@react-types/shared": "3.32.1"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "rimraf": "^5.0.0",
        "tsup": "^8.5.0",
        "typescript": "^5.3.0",
        "vue": "^3.4.0"
    }
}
