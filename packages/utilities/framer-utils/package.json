    },
    "devDependencies": {
        "react": "18.3.0",
        "react-dom": "18.3.0",
        "@types/react": "19.2.2",
        "@types/react-dom": "19.2.2",
        "clean-package": "2.2.0",
        "framer-motion": "11.18.2",
