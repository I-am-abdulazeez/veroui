{
    "name": "@veroui/framer-utils",
    "version": "2.1.23",
    "description": "A set of framer motion transitions for react",
    "keywords": [
        "framer-utils"
    ],
    "author": "VeroUI <support@veroui.com>",
    "homepage": "https://veroui.com",
    "license": "MIT",
    "main": "src/index.ts",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "publishConfig": {
